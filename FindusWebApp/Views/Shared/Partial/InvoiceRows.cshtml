@model List<Fortnox.SDK.Entities.InvoiceRow>
@{
    Layout = null;
}

@if (@Model != null)
{

    <table style="max-width:75%;">
    <tr>
        <th></th>
        <th>SKU</th>
        <th>Kostnad</th>
        <th>Kvantitet</th>
    </tr>
    @foreach (var row in @Model)
        {
            <tr>
                <td style="background-color: lightgreen;"></td>
                <td>@row.ArticleNumber</td>
                <td>@($"{row.Price:F2}")</td><span>(Inkl. Moms)</span>
                <td>@row.DeliveredQuantity</td>
            </tr>
        }
    </table>
}